# =====================================================
# CONFIGURAÇÕES DO BANCO DE DADOS SUPABASE
# =====================================================

# URL do projeto Supabase
SUPABASE_URL=https://your-project.supabase.co

# Chave anônima do Supabase (pública)
SUPABASE_ANON_KEY=your-anon-key-here

# Chave de serviço do Supabase (privada - apenas para operações administrativas)
SUPABASE_SERVICE_KEY=your-service-key-here

# =====================================================
# CONFIGURAÇÕES DO PROJETO
# =====================================================

# ID do projeto padrão (usado quando não especificado)
DEFAULT_PROJECT_ID=PROJ-DEFAULT-001

# =====================================================
# CONFIGURAÇÕES DE LOGGING
# =====================================================

# Nível de log: DEBUG, INFO, WARN, ERROR
LOG_LEVEL=INFO

# Máximo de logs mantidos em memória
MAX_LOGS=1000

# =====================================================
# CONFIGURAÇÕES DE AMBIENTE
# =====================================================

# Ambiente: development, staging, production
NODE_ENV=development

# Porta do servidor (para desenvolvimento local)
PORT=3000

# =====================================================
# CONFIGURAÇÕES DE SEGURANÇA
# =====================================================

# Chave secreta para JWT (se implementado)
JWT_SECRET=your-jwt-secret-here

# Origem permitida para CORS
CORS_ORIGIN=*

# =====================================================
# CONFIGURAÇÕES DE PERFORMANCE
# =====================================================

# Timeout para operações de banco (em ms)
DB_TIMEOUT=30000

# Timeout para análise de agentes (em ms)
AGENT_TIMEOUT=60000

# Máximo de análises em paralelo
MAX_CONCURRENT_ANALYSES=5

# =====================================================
# CONFIGURAÇÕES DE CACHE
# =====================================================

# TTL do cache de análises (em segundos)
ANALYSIS_CACHE_TTL=3600

# TTL do cache de contexto (em segundos)
CONTEXT_CACHE_TTL=1800

# =====================================================
# CONFIGURAÇÕES DE INTEGRAÇÃO
# =====================================================

# URL base para webhooks (se implementado)
WEBHOOK_BASE_URL=https://your-domain.com/webhooks

# Chave de API para integrações externas
EXTERNAL_API_KEY=your-external-api-key

# =====================================================
# CONFIGURAÇÕES DE MONITORAMENTO
# =====================================================

# URL do Sentry para monitoramento de erros (opcional)
SENTRY_DSN=your-sentry-dsn

# Chave de API do New Relic (opcional)
NEW_RELIC_LICENSE_KEY=your-newrelic-key

